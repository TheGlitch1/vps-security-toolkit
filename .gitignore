config/vps-toolkit.local.conf
config/*.secret
*.key
*.pem
*.crt

# Email/Telegram tokens
.env
.env.local

# Test outputs
tests/output/
tests/logs/

# Archive files
*.tar.gz
*.zip
*.tgz

# IDE files
.vscode/
.idea/
*.sublime-*

# OS files
Thumbs.db
desktop.ini

# Compressed logs
*.gz
